--stages

function clear_map()
	for i=0,127 do
		for j=0,63 do
			mset(i,j,0)
		end
	end
end

function load_stage(num)
	
	for i=0,stages[num].h-1 do
		for j=0,stages[num].w do
			p=j+i*(stages[num].w+2)+1
			ob=sub(stages[num].stage,p,p)
			
			if(ob=="1") mset(j,i,9)
			if(ob=="7") then
				ship.x = j*8
				ship.y = i*8
			end
			if(ob=="2") then
				add(coins,
				{x=j*8,y=i*8,
				box={x1=3,y1=3,x2=5,y2=5}})
			end
		end
	end
end

function init_stages()
	
	stages = {}
	
	--[[
		0=blank (also "-")
		1=wall
		2=coin
		3=weakling
		4=speeder
		5=tank
		6=end portal
		7=player
	]]--
	
	add(stages,{
	w=39,h=20,stage=[[
	-1111111111111111111111111111111111111-
	11-----------------------------------11
	1-----------222-----222-----222-------1
	1---22-3-----------4-----------3-22---1
	1--2----222-----222-----222--------2--1
	1--2-------------------------------2--1
	1------1111111111111111111111111------1
	1-----111111111111111111111111111-----1
	1--2--111111111111111111111111111--2--1
	1--2--111111111111111111111111111--2--1
	1--2--111111111111111111111111111--2--1
	1--2--111111111111111111111111111--2--1
	1-----111111111111111111111111111-----1
	1------1111111111111111111111111------1
	1--2--------------111--------------2--1
	1--2--------222----1----222--------2--1
	1---22----------6--1227----------22---1
	1-------222--------1--------222-------1
	11----------------111----------------11
	-1111111111111111111111111111111111111-
	]]
	})
	
end
