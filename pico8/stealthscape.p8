function _init()
	music(0,10000)
	init_ship()
	palette = {bg=1,pl=7,wl=15,en=2}
	coins={}
	crot=1
	
	init_stages()
	clear_map()
	load_stage(1)
	
	pal(15,palette.pl)
	pal(7,palette.wl)
end

function _update60()
	ship_movement()
	
	crot+=1
	if crot > (5*3) then crot = 1 end
	
	for c in all(coins) do
		if(coll(ship,c)) then
			del(coins,c)
			sfx(3)
		end
	end
	
end

function _draw()
 local camx=flr(ship.x)-64+4
 local camy=flr(ship.y)-64+4
	cls()
	rectfill(camx,camy,camx+128,camy+128,palette.bg)
	map()
	camera(camx,camy)
	ship.sp = ship_spr()
	spr(ship.sp,ship.x,ship.y)
	
	for coi in all(coins) do
		spr(9+ceil(crot/5),coi.x,coi.y)
	end
end

function sign(n)
	if n==0 then return 0
	else return sgn(n) end
end
