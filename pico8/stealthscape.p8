function clear_map()
	for i=0,127 do
		for j=0,63 do
			mset(i,j,0)
		end
	end
end

function load_stage(num)
 init_ship()
 coins={}
	win = false
	for i=0,stages[num].h-1 do
		for j=0,stages[num].w do
			p=j+i*(stages[num].w+2)+1
			ob=sub(stages[num].stage,p,p)
			
			if(ob=="1") then
				mset(j,i,9)
			elseif(ob=="7") then
				ship.x = j*8
				ship.y = i*8
			elseif(ob=="6") then
				portal.x = j*8
				portal.y = i*8
			elseif(ob=="2") then
				add(coins,
				{x=j*8,y=i*8,
				box={x1=3,y1=3,x2=5,y2=5}})
			end
		end
	end
end

